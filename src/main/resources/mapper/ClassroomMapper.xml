<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace = "org.example.school.mapper.ClassroomMapper">

    <resultMap id="BaseResultMap" type="org.example.school.entity.Classroom">
            <result property="classroomId" column="classroom_id" jdbcType="BIGINT"/>
            <result property="classroomName" column="classroom_name" jdbcType="VARCHAR"/>
            <result property="classroomPosition" column="classroom_position" jdbcType="VARCHAR"/>
            <result property="createTime" column="create_time" jdbcType="TIMESTAMP"/>
            <result property="updateTime" column="update_time" jdbcType="TIMESTAMP"/>
    </resultMap>


</mapper>
